.global _start

.section .text
    mov $root, %rax
    cmpq $node10, (%rax)
    movq $node10, %rax
    cmpq $10, (%rax)
    cmpq $node1, 8(%rax)
    cmpq $node11, 16(%rax)
    movq $node1, %rax
    cmpq $1, (%rax)
    cmpq $node0, 8(%rax)
    cmpq $node7, 16(%rax)
    movq $node11, %rax
    cmpq $11, (%rax)
    cmpq $new_node, 8(%rax)
    cmpq $node12, 16(%rax)
    movq $node0, %rax
    cmpq $0, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node7, %rax
    cmpq $7, (%rax)
    cmpq $node2, 8(%rax)
    cmpq $node9, 16(%rax)
    movq $new_node, %rax
    cmpq $5, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node12, %rax
    cmpq $12, (%rax)
    cmpq $0, 8(%rax)
    cmpq $node13, 16(%rax)
    movq $node2, %rax
    cmpq $2, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node9, %rax
    cmpq $9, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node13, %rax
    cmpq $13, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node10
node10:
  .quad 10
  .quad node1
  .quad node11
node1:
  .quad 1
  .quad node0
  .quad node7
node11:
  .quad 11
  .quad 0
  .quad node12
node0:
  .quad 0
  .quad 0
  .quad 0
node7:
  .quad 7
  .quad node2
  .quad node9
node12:
  .quad 12
  .quad 0
  .quad node13
node2:
  .quad 2
  .quad 0
  .quad 0
node9:
  .quad 9
  .quad 0
  .quad 0
node13:
  .quad 13
  .quad 0
  .quad 0
new_node: .quad 5, 0, 0
