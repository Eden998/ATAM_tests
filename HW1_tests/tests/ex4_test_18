.global _start

.section .text
    mov $root, %rax
    cmpq $node0, (%rax)
    movq $node0, %rax
    cmpq $0, (%rax)
    cmpq $0, 8(%rax)
    cmpq $node13, 16(%rax)
    movq $node13, %rax
    cmpq $13, (%rax)
    cmpq $node2, 8(%rax)
    cmpq $node14, 16(%rax)
    movq $node2, %rax
    cmpq $2, (%rax)
    cmpq $node1, 8(%rax)
    cmpq $node8, 16(%rax)
    movq $node14, %rax
    cmpq $14, (%rax)
    cmpq $new_node, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node1, %rax
    cmpq $1, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node8, %rax
    cmpq $8, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $new_node, %rax
    cmpq $6, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node0
node0:
  .quad 0
  .quad 0
  .quad node13
node13:
  .quad 13
  .quad node2
  .quad node14
node2:
  .quad 2
  .quad node1
  .quad node8
node14:
  .quad 14
  .quad 0
  .quad 0
node1:
  .quad 1
  .quad 0
  .quad 0
node8:
  .quad 8
  .quad 0
  .quad 0
new_node: .quad 6, 0, 0
