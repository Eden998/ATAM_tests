.global _start

.section .text
    mov $root, %rax
    cmpq $node6, (%rax)
    movq $node6, %rax
    cmpq $6, (%rax)
    cmpq $node1, 8(%rax)
    cmpq $new_node, 16(%rax)
    movq $node1, %rax
    cmpq $1, (%rax)
    cmpq $node0, 8(%rax)
    cmpq $node3, 16(%rax)
    movq $new_node, %rax
    cmpq $11, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node0, %rax
    cmpq $0, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node3, %rax
    cmpq $3, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node6
node6:
  .quad 6
  .quad node1
  .quad 0
node1:
  .quad 1
  .quad node0
  .quad node3
node0:
  .quad 0
  .quad 0
  .quad 0
node3:
  .quad 3
  .quad 0
  .quad 0
new_node: .quad 11, 0, 0
