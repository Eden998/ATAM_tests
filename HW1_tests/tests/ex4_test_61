.global _start

.section .text
    mov $root, %rax
    cmpq $node13, (%rax)
    movq $node13, %rax
    cmpq $13, (%rax)
    cmpq $node0, 8(%rax)
    cmpq $node14, 16(%rax)
    movq $node0, %rax
    cmpq $0, (%rax)
    cmpq $0, 8(%rax)
    cmpq $node10, 16(%rax)
    movq $node14, %rax
    cmpq $14, (%rax)
    cmpq $new_node, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node10, %rax
    cmpq $10, (%rax)
    cmpq $node5, 8(%rax)
    cmpq $node11, 16(%rax)
    movq $new_node, %rax
    cmpq $3, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node5, %rax
    cmpq $5, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $node11, %rax
    cmpq $11, (%rax)
    cmpq $0, 8(%rax)
    cmpq $0, 16(%rax)
    movq $60, %rax
    movq $0, %rdi
    syscall

bad_exit:
    movq $60, %rax
    movq $1, %rdi
    syscall

.section .data
root:
  .quad node13
node13:
  .quad 13
  .quad node0
  .quad node14
node0:
  .quad 0
  .quad 0
  .quad node10
node14:
  .quad 14
  .quad 0
  .quad 0
node10:
  .quad 10
  .quad node5
  .quad node11
node5:
  .quad 5
  .quad 0
  .quad 0
node11:
  .quad 11
  .quad 0
  .quad 0
new_node: .quad 3, 0, 0
